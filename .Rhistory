library(tidyquant)
beta <- function(symbol,index,from,to,period) {
capm_beta <- rename((tq_performance(data = left_join((tq_get(symbol,
get  = "stock.prices",
from = from,
to   = to) %>%
tq_transmute(select     = adjusted,
mutate_fun = periodReturn,
period     = period,
type       = "log",
col_rename = "Ra")),
(tq_get(index,
get  = "stock.prices",
from = from,
to   = to) %>%
tq_transmute(select     = adjusted,
mutate_fun = periodReturn,
period     = period,
type       = "log",
col_rename = "Rb")),
by = c("date" = "date")),
Ra = Ra,
Rb = Rb,
performance_fun = CAPM.beta)), Beta = 'CAPM.beta.1')
chart.Regression(Ra = Ra, Rb = Rb, Rf = 0, excess.returns = FALSE,
reference.grid = TRUE, main = "Scatterplot", ylab = NULL, xlab = NULL,
xlim = NA, colorset = 1:12, symbolset = 1:12,
element.color = "darkgray", legend.loc = NULL, ylog = FALSE,
fit = c("loess", "linear"), ylim = NA,
evaluation = 50, legend.cex = 0.8, cex = 0.8, lwd = 2)
return(capm_beta)
}
